{
  "name": "@esri/solutions-components",
  "version": "0.5.2",
  "description": "Web Components for Esri's Solutions Applications",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/solutions-components/solutions-components.esm.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run lint:ts && stencil build --docs",
    "build:debug": "stencil build -c stencil.debugconfig.ts",
    "start": "stencil build --dev --watch --serve",
    "clean:src": "start cmd.exe /c cleanSrc.bat",
    "lint:ts": "eslint --ext .ts,.tsx src/components src/utils",
    "lint:ts:fix": "eslint --ext .ts,.tsx --fix src/components src/utils",
    "lint:ts:prettier": "prettier --write \"**/*.ts?(x)\"",
    "release:publish": "npm publish --access public && ts-node ./support/releaseToGitHub.ts",
    "test": "stencil test --spec --e2e --detectOpenHandles --coverage --collectCoverageFrom=src/utils/*.ts",
    "test:clean": "npm run clean:src && npm run lint:ts:fix && stencil test --spec --e2e --detectOpenHandles",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "update-browserslist": "npx browserslist@latest --update-db"
  },
  "devDependencies": {
    "@esri/eslint-plugin-calcite-components": "^0.2.2",
    "@esri/solution-common": "1.5.3",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^2.0.0",
    "@stencil/store": "^2.0.1",
    "@types/arcgis-js-api": "^4.25.0-next.20221031",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.11",
    "@types/puppeteer": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "10.4.4",
    "caniuse-lite": "^1.0.30001412",
    "dotenv": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-unicorn": "39.0.0",
    "esri-loader": "3.4.0",
    "form-request-submit-polyfill": "^2.0.0",
    "gh-release": "^6.0.3",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "pify": "^5.0.0",
    "prettier": "2.4.1",
    "puppeteer": "^13.5.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "3.0.23",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@pdf-lib/fontkit": "^1.1.1",
    "@stencil/core": "^2.20.0",
    "focus-trap": "7.2.0",
    "jspdf": "^2.5.1",
    "minimist": "^1.2.6",
    "pdf-lib": "^1.17.1"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-auth": "^3.4.2",
    "@esri/arcgis-rest-feature-layer": "^3.4.2",
    "@esri/arcgis-rest-portal": "^3.4.2",
    "@esri/arcgis-rest-request": "^3.4.2",
    "@esri/arcgis-rest-service-admin": "^3.4.2",
    "@esri/calcite-components": "^1.0.7",
    "@esri/hub-common": "9.29.0",
    "@esri/hub-initiatives": "9.29.0",
    "@esri/hub-sites": "9.29.0",
    "@esri/hub-teams": "9.29.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/solutions-components.git"
  },
  "contributors": [
    {
      "name": "John Hauck",
      "email": "jhauck@esri.com"
    },
    {
      "name": "Mike Tschudi",
      "email": "mtschudi@esri.com"
    }
  ],
  "license": "Apache-2.0",
  "volta": {
    "node": "16.17.1",
    "npm": "8.18.0",
    "typescript": "4.8.4"
  }
}
