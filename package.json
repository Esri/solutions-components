{
  "name": "@esri/solutions-components",
  "version": "0.0.10",
  "description": "Web Components for Esri's Solutions Applications",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.js",
  "es2017": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/solutions-components/solutions-components.js",
  "files": [
    "dist/"
  ],
  "browserslist": [
    "last 2 and_chr versions",
    "last 2 and_ff versions",
    "last 2 android versions",
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "last 2 ios versions",
    "last 2 safari versions"
  ],
  "scripts": {
    "prepare": "is-ci || husky install",
    "analyze": "ANALYZE_BUNDLES=1 stencil build",
    "build": "npx browserslist@latest --update-db && start/b cmd.exe /c \"src\\testingAssets\\nlsJStoJSON src\\assets\\nls\\strings.js src\\testingAssets\" && stencil build",
    "build:debug": "npx browserslist@latest --update-db && start/b cmd.exe /c \"src\\testingAssets\\nlsJStoJSON src\\assets\\nls\\strings.js src\\testingAssets\" && stencil build -c stencil.debugconfig.ts",
    "build.watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "lint:test": "npm run lint && stencil test --spec --e2e",
    "test": "stencil test --spec --e2e",
    "test:clean": "npm run clean:src && npm run lint:fix && stencil test --spec --e2e --detectOpenHandles",
    "test.watch": "stencil test --spec --e2e --watch",
    "test:watch:spec": "stencil test --spec --watch",
    "test:watch:e2e": "stencil test --e2e --watch",
    "test:ci:spec": "stencil test --spec",
    "test:ci:e2e": "stencil test --e2e --max-workers=2",
    "test:ci:e2e:qaext": "ENV=qaext npm run test:ci:e2e",
    "test:ci:e2e:devext": "ENV=devext npm run test:ci:e2e",
    "test:ci:e2e:prod": "ENV=prod npm run test:ci:e2e",
    "clean": "rimraf dist loader www",
    "clean:src": "rimraf src/components/*/*.{d.ts,js,js.map} && rimraf src/components/*/test/*.{d.ts,js,js.map} && rimraf src/utils/*.{d.ts,js,js.map}",
    "generate": "stencil generate && plop generate",
    "generate-story": "plop story",
    "generate-harness": "plop harness",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:s": "standardx --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin src/**/*.ts*",
    "lint-fix:s": "standardx --fix --parser @typescript-eslint/parser --plugin @typescript-eslint/eslint-plugin src/**/*.ts*",
    "publish-npm": "npm publish --access public",
    "prepublishOnly": "rimraf node_modules/@esri/solution-*/dist/es2017 && npm run clean && npm run build",
    "release:beta": "npm version prerelease --preid beta -f && git push origin master --tags && ./node_modules/.bin/gh-release --prerelease",
    "release:publish": "npm publish --access public && ts-node ./support/releaseToGitHub.ts",
    "build-storybook": "build-storybook -s ./dist,./public",
    "storybook.run": "run-s build start-storybook",
    "storybook": "npm-run-all build --parallel build.watch start-storybook",
    "start-storybook": "start-storybook -p 6006 -s ./dist,./public --no-manager-cache",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@a11y/focus-trap": "^1.0.5",
