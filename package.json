{
  "name": "solutions-components",
  "version": "0.0.1",
  "description": "Web Components for Esri's Solutions Applications",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/solutions/solutions.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "start/b cmd.exe /c \"src\\testingAssets\\nlsJStoJSON src\\assets\\nls\\strings.js src\\testingAssets\" && stencil build",
    "build:debug": "start/b cmd.exe /c \"src\\testingAssets\\nlsJStoJSON src\\assets\\nls\\strings.js src\\testingAssets\" && stencil build -c stencil.debugconfig.ts",
    "start": "stencil build --dev --watch --serve",
    "clean:src": "start cmd.exe /c cleanSrc.bat",
    "test": "stencil test --spec --e2e --detectOpenHandles",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^11.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@esri/calcite-components": "^1.0.0-beta.69",
    "@stencil/core": "2.11.0",
    "@stencil/store": "^1.5.0",
    "monaco-editor": "^0.30.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcGIS/solutions-components.git"
  },
  "contributors": [
    {
      "name": "John Hauck",
      "email": "jhauck@esri.com"
    },
    {
      "name": "Mike Tschudi",
      "email": "mtschudi@esri.com"
    }
  ],
  "license": "Apache-2.0",
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.0",
    "typescript": "4.4.4"
  }
}
